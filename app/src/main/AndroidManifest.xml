<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.retrofitrxpo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.Light">
        <activity android:name="com.google.qike.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.google.qike.ListViewDemo"/>
        <activity android:name="com.google.qike.GrideViewDemo"/>
        <activity android:name="com.google.qike.RecyclerViewDemo"/>
    </application>

</manifest>